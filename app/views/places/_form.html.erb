<%= form_for @place do |f| %>
<p>
  <%= f.label :name %><br>
  <%= f.text_field :name %>
</p>
<p>
  <%= f.label :address %><br>
  <%= f.text_field :address %>
</p>
<p>
  <%= f.label :city %><br>
  <%= f.text_field :city %>
</p>
<p>
  <%= f.label :state %><br>
  <%= f.text_field :state %>
</p>
<p>
  <%= f.label :zipcode %><br>
  <%= f.text_field :zipcode %>
</p>
<p>
  <%= f.label :phone %><br>
  <%= f.text_field :phone %>
</p>
<p>
  <%= f.label :website %><br>
  <%= f.text_field :website %>
</p>

<p>
  <%= f.label :category %><br>
  <%= f.collection_select :category_id, Category.all, :id, :name %>
</p>

<p>
  <%= f.label :description %><br>
  <%= f.text_field :description %>
</p>
<p>
  <%= f.submit %>
</p>
<% end %>
